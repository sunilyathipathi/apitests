{
	"$schema": "http://json-schema.org/draft-04/schema#",
	"type": "object",
	"properties": {
		"books": {
			"type": "array",
			"uniqueItems": true,
			"minItems": 1,
			"maxItems": 10,
			"items": [
				{
					"type": "object",
					"properties": {
						"isbn": {
							"type": "string"
						},
						"title": {
							"type": "string"
						},
						"subTitle": {
							"type": "string"
						},
						"author": {
							"type": "string"
						},
						"publish_date": {
							"type": "string"
						},
						"publisher": {
							"type": "string"
						},
						"pages": {
							"type": "integer"
						},
						"description": {
							"type": "string"
						},
						"website": {
							"type": "string"
						}
					},
					"required": [
						"isbn",
						"title",
						"subTitle",
						"author",
						"publish_date",
						"publisher",
						"pages",
						"description",
						"website"
					]
				}
			]
		}
	},
	"required": [
		"books"
	]
}